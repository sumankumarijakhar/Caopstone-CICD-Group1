{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["es2020"],
    "declaration": false,
    "sourceMap": true,
    "outDir": "cdk.out",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "types": ["node", "aws-lambda"]
  },
  "include": ["bin", "lib", "backend"]
}
